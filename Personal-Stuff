function loading(boolean, seconds)
if boolean then
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
screenGui.Name = "LoadingScreen"
local blackBackground = Instance.new("Frame")
blackBackground.Size = UDim2.new(2, 0, 2, 0)
blackBackground.Position = UDim2.new(0, 0, 0, 0)
blackBackground.BackgroundColor3 = Color3.new(0, 0, 0)
blackBackground.BorderSizePixel = 0
blackBackground.Parent = screenGui
local loadingText = Instance.new("TextLabel")
loadingText.Size = UDim2.new(0, 400, 0, 100)
loadingText.Position = UDim2.new(0.5, -200, 0.5, -50)
loadingText.BackgroundTransparency = 1
loadingText.Text = "Loading..."
loadingText.TextColor3 = Color3.new(1, 1, 1)
loadingText.TextScaled = true
loadingText.Font = Enum.Font.Arcade
loadingText.Parent = screenGui
local smallText = Instance.new("TextLabel")
smallText.Size = UDim2.new(0, 300, 0, 50)
smallText.Position = UDim2.new(0.5, -150, 0.5, 60)
smallText.BackgroundTransparency = 1
smallText.Text = "This May Take A While"
smallText.TextColor3 = Color3.new(1, 1, 1)
smallText.TextScaled = true
smallText.Font = Enum.Font.Arcade
smallText.Parent = screenGui
local bottomText = Instance.new("TextLabel")
bottomText.Size = UDim2.new(0, 300, 0, 30)
bottomText.Position = UDim2.new(0, 10, 1, -40)
bottomText.BackgroundTransparency = 1
bottomText.Text = ""
bottomText.TextColor3 = Color3.new(1, 1, 1)
bottomText.TextScaled = true
bottomText.Font = Enum.Font.Arcade
bottomText.TextXAlignment = Enum.TextXAlignment.Left
bottomText.Parent = screenGui
local loadingTexts = {
"Loading Accessories",
"Loading Ultimate",
"Loading Skill Names",
"Loading Texts",
"Loading Fucks",
"Loading Assets",
"Loading Environment",
"Loading Animations",
"Loading UI Components",
"Loading Player Data",
"Loading Visual Effects",
"Loading Backgrounds",
"Loading Scripts",
"Loading Audio",
"Loading Physics",
"Loading Tools",
"Loading Particles",
"Added Care To Our Players",
"Fixing Errors",
"Loading Anti-IP Logger"
}
spawn(function()
while true do
for _, text in ipairs(loadingTexts) do
bottomText.Text = text
wait(1)
end
end
end)
local tweenService = game:GetService("TweenService")
local bounceUp = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
local bounceDown = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
local function bounceText()
local upTween = tweenService:Create(loadingText, bounceUp, {Position = UDim2.new(0.5, -200, 0.5, -70)})
local downTween = tweenService:Create(loadingText, bounceDown, {Position = UDim2.new(0.5, -200, 0.5, -50)})
upTween:Play()
upTween.Completed:Connect(function()
downTween:Play()
end)
end
spawn(function()
while true do
bounceText()
wait(1)
end
end)
wait(seconds)
screengui:Destroy()
else
print([[
()--------------------------------------------------()
()i set the loading screen off because it is boring.()
()--------------------------------------------------()
]])
end
end
